xml\/xml.min.js\"><\/script>\r\n  <script src=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/codemirror\/5.65.16\/mode\/javascript\/javascript.min.js\"><\/script>\r\n  <script src=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/codemirror\/5.65.16\/mode\/css\/css.min.js\"><\/script>\r\n  <script src=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/codemirror\/5.65.16\/addon\/edit\/closetag.min.js\"><\/script>\r\n  <script src=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/codemirror\/5.65.16\/addon\/edit\/closebrackets.min.js\"><\/script>\r\n  <script>\r\n    const htmlTemplate = `<!DOCTYPE html>\r\n<html>\r\n<head>\r\n  <title>My Page<\/title>\r\n<\/head>\r\n<body>\r\n  <h1>Hello World<\/h1>\r\n<\/body>\r\n<\/html>`;\r\n\r\nconst cssTemplate = `body {\r\n  background-color: #f0f0f0;\r\n  font-family: Arial, sans-serif;\r\n  color: #333;\r\n}`;\r\n\r\nconst jsTemplate = `console.log(\"Hello from JS!\");`;\r\n\r\nlet currentLang = 'html';\r\nlet codeData = {\r\n  html: htmlTemplate,\r\n  css: cssTemplate,\r\n  js: jsTemplate\r\n};\r\n\r\n\/\/ Initialize CodeMirror\r\nconst editor = CodeMirror.fromTextArea(document.getElementById(\"lwacode-editor\"), {\r\n  mode: \"xml\",\r\n  theme: \"material-darker\",\r\n  lineNumbers: true,\r\n  autoCloseTags: true,\r\n  autoCloseBrackets: true\r\n});\r\neditor.setValue(codeData.html);\r\n\r\n\/\/ Tab Switching\r\ndocument.querySelectorAll('.lwacode-tab').forEach(tab => {\r\n  tab.addEventListener('click', () => {\r\n    document.querySelectorAll('.lwacode-tab').forEach(t => t.classList.remove('active'));\r\n    tab.classList.add('active');\r\n    currentLang = tab.getAttribute('data-lang');\r\n    const mode = currentLang === 'html' ? 'xml' : currentLang === 'css' ? 'css' : 'javascript';\r\n    editor.setOption('mode', mode);\r\n    editor.setValue(codeData[currentLang]);\r\n  });\r\n});\r\n\r\n\/\/ Run Output\r\ndocument.querySelector('.lwacode-btn-run').addEventListener('click', () => {\r\n  codeData[currentLang] = editor.getValue();\r\n  const result = `\r\n    <html>\r\n    <head><style>${codeData.css}<\/style><\/head>\r\n    <body>\r\n      ${codeData.html}\r\n      <script>${codeData.js}<\\\/script>\r\n    <\/body>\r\n    <\/html>\r\n  `;\r\n  document.getElementById(\"lwacode-output-frame\").srcdoc = result;\r\n});\r\n\r\n\/\/ Clear Output\r\ndocument.getElementById(\"lwacode-clear-output\").addEventListener(\"click\", () => {\r\n  document.getElementById(\"lwacode-output-frame\").srcdoc = \"\";\r\n});\r\n\r\n\/\/ Theme Toggle\r\ndocument.getElementById(\"lwacode-theme-toggle\").addEventListener(\"click\", () => {\r\n  document.body.classList.toggle(\"light\");\r\n  const icon = document.querySelector(\"#lwacode-theme-toggle i\");\r\n  icon.className = document.body.classList.contains(\"light\") ? \"fas fa-sun\" : \"fas fa-moon\";\r\n});\r\n\r\n\/\/ Fullscreen Preview\r\ndocument.getElementById('lwacode-fullscreen-btn').addEventListener('click', () => {\r\n  const iframe = document.getElementById(\"lwacode-output-frame\");\r\n\r\n  const fullscreenDiv = document.createElement(\"div\");\r\n  fullscreenDiv.className = \"lwacode-fullscreen-preview\";\r\n\r\n  const exitBtn = document.createElement(\"button\");\r\n  exitBtn.className = \"lwacode-exit-fullscreen\";\r\n  exitBtn.innerHTML = '<i class=\"fas fa-times\"><\/i>';\r\n\r\n  const fullIframe = document.createElement(\"iframe\");\r\n  fullIframe.srcdoc = iframe.srcdoc;\r\n  fullIframe.className = \"lwacode-fullscreen-iframe\";\r\n\r\n  fullscreenDiv.appendChild(exitBtn);\r\n  fullscreenDiv.appendChild(fullIframe);\r\n  document.body.appendChild(fullscreenDiv);\r\n\r\n  exitBtn.addEventListener(\"click\", () => {\r\n    document.body.removeChild(fullscreenDiv);\r\n    \r\n\r\n\r\n  });\r\n});\r\n\r\n\r\ndocument.getElementById(\"lwacode-download-btn\").addEventListener(\"click\", () => {\r\n  const blob = new Blob([editor.getValue()], { type: \"text\/plain;charset=utf-8\" });\r\n  const a = document.createElement(\"a\");\r\n  a.href = URL.createObjectURL(blob);\r\n  a.download = `code.${currentLang}`;\r\n  a.click();\r\n  URL.revokeObjectURL(a.href);\r\n});\r\n\r\n\r\n  <\/script>\r\n<\/body>\r\n<\/html>\r\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>HTML\/CSS\/JS Code Editor Code Editor HTML\/CSS\/JS Run Clear Download index.html styles.css script.js Output Preview How to Use: Select a file tab: index.html, styles.css, or script.js. Edit the code in the&hellip;<\/p>\n","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"open","ping_status":"closed","template":"elementor_header_footer","meta":{"footnotes":""},"class_list":["post-3156","page","type-page","status-publish","hentry"],"yoast_head":"<!-- This site is optimized with the Yoast SEO plugin v25.9 - https:\/\/yoast.com\/wordpress\/plugins\/seo\/ -->\n<title>Code Editor - Learn With Arshyan<\/title>\n<meta name=\"description\" content=\"Try our interactive HTML\/CSS\/JS code editor with live preview, light\/dark mode, and fullscreen support. Run, clear, or download code.\" \/>\n<meta name=\"robots\" content=\"index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1\" \/>\n<link rel=\"canonical\" href=\"https:\/\/learnwitharshyan.com\/code-editor\/\" \/>\n<meta property=\"og:locale\" content=\"en_US\" \/>\n<meta property=\"og:type\" content=\"article\" \/>\n<meta property=\"og:title\" content=\"Code Editor - Learn With Arshyan\" \/>\n<meta property=\"og:description\" content=\"Try our interactive HTML\/CSS\/JS code editor with live preview, light\/dark mode, and fullscreen support. Run, clear, or download code.\" \/>\n<meta property=\"og:url\" content=\"https:\/\/learnwitharshyan.com\/code-editor\/\" \/>\n<meta property=\"og:site_name\" content=\"Learn With Arshyan\" \/>\n<meta property=\"article:modified_time\" content=\"2025-07-15T13:09:35+00:00\" \/>\n<meta name=\"twitter:card\" content=\"sum