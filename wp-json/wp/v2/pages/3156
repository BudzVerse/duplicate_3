creen-btn {\r\n  background: #5e17ab;\r\n  color: white;\r\n}\r\n\r\nbody.light .lwacode-fullscreen-btn:hover {\r\n  background: #7d3cc4;\r\n}\r\n\r\n\/* Responsive: Tablet & Mobile *\/\r\n@media screen and (max-width: 768px) {\r\n  .lwacode-header {\r\n    flex-direction: column;\r\n    align-items: flex-start;\r\n    gap: 15px;\r\n  }\r\n\r\n  .lwacode-header-controls {\r\n    flex-direction: row;\r\n    flex-wrap: wrap;\r\n    gap: 10px;\r\n    width: 100%;\r\n  }\r\n\r\n  .lwacode-tabs {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .lwacode-tab {\r\n    border-right: none;\r\n    border-bottom: 1px solid #3a3c52;\r\n  }\r\n\r\n  .lwacode-editor-container {\r\n    height: 250px;\r\n  }\r\n\r\n  .lwacode-output-frame {\r\n    height: 200px;\r\n  }\r\n\r\n  .lwacode-instructions {\r\n    font-size: 14px;\r\n    padding: 12px 15px;\r\n  }\r\n\r\n  .lwacode-fullscreen-btn {\r\n    width: 36px;\r\n    height: 36px;\r\n    font-size: 0.9rem;\r\n  }\r\n\r\n  .lwacode-exit-fullscreen {\r\n    top: 10px;\r\n    right: 10px;\r\n    padding: 6px 10px;\r\n    font-size: 1rem;\r\n  }\r\n}\r\n\r\n<\/style>\r\n\r\n  <!-- FontAwesome for icons -->\r\n  <link rel=\"stylesheet\" href=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/font-awesome\/6.5.0\/css\/all.min.css\"\/>\r\n\r\n  <!-- CodeMirror CSS -->\r\n  <link rel=\"stylesheet\" href=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/codemirror\/5.65.16\/codemirror.min.css\">\r\n  <link rel=\"stylesheet\" href=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/codemirror\/5.65.16\/theme\/material-darker.min.css\">\r\n<\/head>\r\n<body>\r\n  <div class=\"lwacode-container\">\r\n    <header class=\"lwacode-header\">\r\n      <div class=\"lwacode-logo\">Code Editor <span>HTML\/CSS\/JS<\/span><\/div>\r\n      <div class=\"lwacode-header-controls\">\r\n        <button id=\"lwacode-theme-toggle\" class=\"lwacode-btn\"><i class=\"fas fa-moon\"><\/i><\/button>\r\n        <button class=\"lwacode-btn lwacode-btn-run\">Run<\/button>\r\n        <